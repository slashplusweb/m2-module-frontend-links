{
    "name": "slashplus/module-frontend-links",
    "description": "Add product frontend links to Magento 2 administration",
    "keywords": [
        "slashplus",
        "module-frontend-links",
        "magento2"
    ],
    "type": "magento2-module",
    "homepage": "https://github.com/slashplusweb/m2-module-frontend-links",
    "license": "MIT",
    "authors": [
        {
            "name": "Emanuel Jacob",
            "email": "info@slashplus.de",
            "homepage": "https://www.slashplus.de"
        }
    ],
    "version": "0.1.0",
    "require": {
        "php": "^7.4",
        "magento/module-catalog": ">=104"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Slashplus\\FrontendLinks\\": ""
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
